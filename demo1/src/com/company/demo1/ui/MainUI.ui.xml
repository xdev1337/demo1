<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>

<?xdevuiml version="1.0"?>
<?import com.company.demo1.ui.MainView?>
<?import com.xdev.ui.XdevButton?>
<?import com.xdev.ui.XdevGridLayout?>
<?import com.xdev.ui.XdevUI?>
<?import com.xdev.ui.navigation.XdevNavigator?>
<XdevUI xmlns:x="http://xml.xdev-software.com/xdevuiml" x:name="MainUI">
    <x:constraints designWidth="600" zpos="0" designHeight="400" width="100%" height="100%" />
    <XdevGridLayout x:name="gridLayout">
        <x:constraints zpos="0" width="100%" height="100%" />
        <XdevButton caption="Button" x:name="button">
            <x:constraints gridx="1" gridy="1" weighty="0.0" weightx="0.0" />
        </XdevButton>
        <XdevButton caption="Button" x:name="button2">
            <x:constraints gridx="2" gridy="2" weighty="0.0" weightx="0.0" />
            <x:event>click.buttonClick</x:event>
        </XdevButton>
        <XdevButton caption="Button" x:name="button3">
            <x:constraints gridx="3" gridy="3" weighty="0.0" weightx="0.0" />
        </XdevButton>
    </XdevGridLayout>
    <XdevNavigator x:name="navigator" x:singleComponentContainer="MainUI">
        <x:views>
            <entry path="" class="MainView" />
        </x:views>
    </XdevNavigator>
</XdevUI>

