<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>

<?xdevuiml version="1.0"?>
<?import com.company.demo1.entities.Employees?>
<?import com.xdev.ui.XdevButton?>
<?import com.xdev.ui.XdevGridLayout?>
<?import com.xdev.ui.XdevView?>
<?import com.xdev.ui.entitycomponent.table.XdevTable?>
<XdevView xmlns:x="http://xml.xdev-software.com/xdevuiml" x:name="MainView">
    <x:constraints designWidth="600" zpos="0" designHeight="400" width="100%" height="100%" />
    <XdevGridLayout x:name="gridLayout">
        <x:constraints zpos="0" width="100%" height="100%" />
        <XdevButton x:name="button">
            <x:constraints gridx="1" gridy="1" weighty="0.0" weightx="0.0" />
        </XdevButton>
        <XdevButton caption="Cool" x:name="button2">
            <x:constraints gridx="2" gridy="2" weighty="0.0" weightx="0.0" />
            <x:event>click.buttonClick</x:event>
        </XdevButton>
        <XdevButton caption="Button" x:name="button3">
            <x:constraints gridx="3" gridy="3" weighty="0.0" weightx="0.0" />
        </XdevButton>
        <XdevTable x:name="table" x:entity="Employees">
            <x:entityProperties>
                <entry type="int" property="employeeId" />
            </x:entityProperties>
            <x:constraints gridx="4" gridy="4" weighty="1.0" weightx="1.0" width="100%" height="100%" />
            <x:typeArguments>
                <typeArgument type="Employees" />
            </x:typeArguments>
        </XdevTable>
    </XdevGridLayout>
</XdevView>

